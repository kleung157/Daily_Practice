## Check datatypes, dimensions, duplicates, nulls, and unique value counts:
# Dimensions -
# Duplicates -
# Nulls -
# Value counts -

# R
data.frame(lapply(df, class))
dim(df)
sum(duplicated(df))
enframe(colSums(is.na(df)), name="index", value="null_count")
enframe(table(df$col), name="index", value="value_count")

# Python
df.info()
df.shape
df.duplicated().sum()
df.isna().sum().reset_index()
df['col'].value_counts().reset_index()

