-- Check dimensions, duplicates, nulls, and unique value counts:
-- Dimensions -
-- Duplicates -
-- Nulls -
-- Value Counts -

SELECT -- Dimensions and nulls
    SUM(CASE WHEN col1 is NULL THEN 1 ELSE 0 END) AS col1_null_count,
    SUM(CASE WHEN col2 is NULL THEN 1 ELSE 0 END) AS col2_null_count,
    COUNT(*) AS total_rows
FROM table;

SELECT -- Duplicates
    col1, col2
    COUNT(*) AS duplicate_count
FROM table
GROUP BY col1, col2
HAVING COUNT(*) > 1;

SELECT -- Value counts
    col1,
    COUNT(*) AS frequency
FROM table
GROUP BY col1
ORDER BY frequency DESC;

